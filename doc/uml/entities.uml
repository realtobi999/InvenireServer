@startuml Invenire Entities Relationships
class Admin {
	+ Guid Id
	+ string EmailAddress
	+ string Password
	+ LoginLock LoginLock
	+ Datetime LastLoginAt
	+ Datetime UpdatedAt
	+ Datetime CreatedAt

	/'Entity Framework Navigation Properties'/
	+ Guid OrganizationId
}

class Employee {
	+ Guid Id
	+ List<PropertyItem> AssignedItems

	/'Entity Framework Navigation Properties'/
	+ Guid OrganizationId
}

class Organization {
	+ Guid Id
	+ string Name
	+ Datetime UpdatedAt
	+ Datetime CreatedAt
}

class Property {
	+ Guid Id
	+ List<PropertyGroup> Groups

	/'Entity Framework Navigation Properties'/
	+ Guid OrganizationId
}

class PropertyCheck {
	+ Guid Id
	+ List<PropertyItem> ScannedItems

	/'Entity Framework Navigation Properties'/
	+ Guid? PropertyId
	+ Guid? OrganizationId
}

class PropertyGroup {
	+ Guid Id
	+ List<PropertyItem> Items
	+ List<PropertyGroup> Subs

	/'Entity Framework Navigation Properties'/
	+ Guid PropertyId
	+ Guid? ParentGroupId
}

class PropertyItem {
	+ Guid Id

	/'Entity Framework Navigation Properties'/
	+ Guid PropertyGroupId
}

/'Relationships'/
Organization --> Admin : has >
Organization --> Employee : employs >
Organization --> Property : manages >
Organization --> PropertyCheck : audits >

Property --> PropertyGroup : has >
Property --> PropertyCheck : checked in >

PropertyCheck --> PropertyItem : scans >

PropertyGroup --> PropertyItem : contains >
PropertyGroup --> PropertyGroup : sub-group >
PropertyGroup --> Property : belongs to >
@enduml
